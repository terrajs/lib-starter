{
  "name": "@terrajs/lib-starter",
  "version": "1.0.0",
  "description": "Starter to create libs",
  "main": "lib/",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint 'lib/*.js'",
    "test": "npm run lint && nyc ava --verbose --serial test/ && nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "preversion": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terrajs/lib-starter.git"
  },
  "nyc": {
    "include": [
      "lib/"
    ]
  },
  "contributors": [
    {
      "name": "Sebastien Chopin (@Atinux)"
    },
    {
      "name": "Benjamin Canac (@benjamincanac)"
    }
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "ava": "0.25.0",
    "codecov": "2.3.1",
    "eslint": "4.7.2",
    "nyc": "11.2.1",
    "rimraf": "2.6.2",
    "std-mocks": "1.0.1"
  },
  "keywords": []
}
